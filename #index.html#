	<section class="container"> <!-- SYNTAX LISTS -->
	    
	    <div class="list-group p-t-2">
		<div class="col-md-4">
		    <h4 class="list-group-item-heading bg-danger">Child:</h4>
		    <p class="list-group-item-text p-l-1">
			<strong>nav>ul>li</strong><br>
			&lt;nav&gt;<br>&nbsp;&nbsp;
			&lt;ul&gt;<br>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
			&lt;li&gt;<br>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
			&lt;/li&gt;<br>&nbsp;&nbsp;
			&lt;/ul&gt;<br>
			&lt;/nav&gt;
		    </p>
		</div>
		<div class="col-md-4">		
		    <h4 class="list-group-item-heading bg-danger">Sibling:</h4>
		    <p class="list-group-item-text p-l-1 p-b-2">
			<strong>div+p+bq</strong><br>		    
			&lt;div&gt;&lt;/div&gt;<br>
			&lt;p&gt;&lt;/p&gt;<br>
			&lt;blockquote&gt;&lt;/blockquote&gt;<br>
		    </p>
		</div>
		<div class="col-md-4">
		    <h4 class="list-group-item-heading bg-danger">Climb-up</h4>
		    <p class="list-group-item-text p-l-1 p-b-2">
			<strong>div>p>span+em^bq</strong><br>
			&lt;div&gt;&lt;/div&gt;<br>
			&lt;div&gt;<br>&nbsp;&nbsp;
			&lt;p&gt;&lt;span&gt;&lt;/span&gt;&lt;em&gt;&lt;/em&gt;&lt;/p&gt;<br>&nbsp;&nbsp;
			&lt;blockquote&gt;&lt;/blockquote&gt;<br>
			&lt;/div&gt;
		    </p>
		</div>
		<div class="col-md-4">
		    <h4 class="list-group-item-heading bg-danger">Grouping:</h4>
		    <p class="list-group-item-text p-l-1 p-b-2">
			<strong>div>(header>ul>li*2>a)+footer>p</strong><br>
			&lt;div&gt;<br>&nbsp;&nbsp;
		        &lt;header&gt;<br>&nbsp;&nbsp;&nbsp;&nbsp;
			&lt;ul&gt;<br>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
		        &lt;li&gt;&lt;a href=""&gt;&lt;/a&gt;&lt;/li&gt;<br>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
		        &lt;li&gt;&lt;a href=""&gt;&lt;/a&gt;&lt;/li&gt;<br>&nbsp;&nbsp;&nbsp;&nbsp;
			&lt;/ul&gt<br>&nbsp;&nbsp;
		        &lt;/header&gt;<br>&nbsp;&nbsp;
		        &lt;footer&gt;<br>&nbsp;&nbsp;&nbsp;&nbsp;
			&lt;p&gt;&lt;/p&gt;<br>&nbsp;&nbsp;
		        &lt;/footer&gt;<br>
			&lt;/div&gt;
		    </p>
		</div>
		<div class="col-md-4">
		    <h4 class="list-group-item-heading bg-danger">Multiplication:</h4>
		    <p class="list-group-item-text p-l-1 p-b-2">
			<strong>ul>li*4<br></strong>
			&lt;ul&gt;<br>&nbsp;&nbsp;
		        &lt;li&gt;&lt;/li&gt;<br>&nbsp;&nbsp;
		        &lt;li&gt;&lt;/li&gt;<br>&nbsp;&nbsp;
		        &lt;li&gt;&lt;/li&gt;<br>&nbsp;&nbsp;
		        &lt;li&gt;&lt;/li&gt;<br>&nbsp;&nbsp;
			&lt;/ul&gt;<br><br>
			
			<strong>h$[title=item$]{Header $}*3</strong><br>			
			&lt;h1 title="item1"&gt;Header 1&lt;/h1&gt;<br>
			&lt;h2 title="item2"&gt;Header 2&lt;/h2&gt;<br>
			&lt;h3 title="item3"&gt;Header 3&lt;/h3&gt;<br>
		    </p>
		</div>
		<div class="col-md-4">
		    <h4 class="list-group-item-heading bg-danger">Id and Class attribute</h4>
		    <p class="list-group-item-text p-l-1 p-b-2">
			<strong>header</strong><br>
			&lt;div id="header"&gt;&lt;/div&gt;<br>
			<strong>.title</strong><br>
			&lt;div class="title"&gt;&lt;/div&gt;<br>
			<strong>form#search.wide</strong><br>
			&lt;form id="search" class="wide"&gt;&lt;/form&gt;<br>
			<strong>p.class1.class2.class3</strong><br>
			&lt;p class="class1 class2 class3"&gt;&lt;/p&gt;<br>
		    </p>
		</div>
	    </div> <!-- END OF  -->
